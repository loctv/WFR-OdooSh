<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="as_assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/product_default_code/static/src/js/custom_product.js" />
        </xpath>
    </template>
    <template id="as_product" inherit_id="website_sale.product">
        <xpath expr="//h1[@t-field='product.name']" position="after">
            <div class="product_default_code"/>
        </xpath>
    </template>
</odoo>
